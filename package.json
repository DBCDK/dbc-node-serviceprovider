{
  "name": "dbc-node-serviceprovider",
  "version": "0.0.2",
  "description": "Provider for the Node modules that accesesthe DBC webservices",
  "main": "index.js",
  "homepage": "https://github.com/DBCDK/dbc-node-serviceprovider/tree/master#readme",
  "scripts": {
    "build": "npm run cleardest && babel ./src -d dist --ignore __tests__",
    "cleardest": "rm -rf ./dist",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "reinstall": "rm -r node_modules && npm install",
    "test": "mocha --compilers js:babel/register src/**/__tests__/*.test.js  -R mocha-jenkins-reporter",
    "test:junit": "npm run test",
    "test:watch": "mocha --compilers js:babel/register src/**/__tests__/*.test.js -w",
    "watch": "npm run cleardest && babel ./src --watch -d dist --ignore __tests__ --retain-lines"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/dbc-node-serviceprovider.git#master"
  },
  "bugs": {
    "url": "https://github.com/DBCDK/dbc-node-serviceprovider/issues"
  },
  "keywords": [
    "dbc",
    "webservice",
    "provider",
    "ws"
  ],
  "author": "dbcdk",
  "license": "GPL-3.0",
  "dependencies": {
    "cache-manager": "^1.0.0",
    "dbc-node-moreinfo-client": "^1.0.1",
    "dbc-node-openagency-client": "git+https://github.com/DBCDK/dbc-node-openagency-client#stable_dist",
    "dbc-node-openorder-client": "^1.0.1",
    "dbc-node-opensearch-client": "^1.0.1",
    "dbc-node-opensuggest": "git+https://github.com/DBCDK/dbc-node-opensuggest#stable_dist",
    "dbc-node-popsuggest": "git+https://github.com/DBCDK/dbc-node-popsuggest#stable_dist",
    "dbc-node-profile-client": "git+https://github.com/DBCDK/dbc-node-profile-client#stable_dist",
    "dbc-node-recommendations": "git+https://github.com/DBCDK/dbc-node-recommendations#stable_dist",
    "jsonpath-object-transform": "https://github.com/vibjerg/jsonpath-object-transform/archive/1.0.5.tar.gz",
    "lodash": "^3.10.1",
    "sinon": "^1.16.1",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "babel-eslint": "^4.0.5",
    "chai": "^3.0.0",
    "eslint": "^1.0.0",
    "mocha": "^2.2.5",
    "mocha-jenkins-reporter": "^0.1.9",
    "node-libs-browser": "^0.5.2",
    "sinon": "^1.15.0",
    "webpack": "^1.9.10"
  }
}
