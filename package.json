{
  "name": "dbc-node-serviceprovider",
  "version": "0.0.1",
  "description": "Provider for the Node modules that accesesthe DBC webservices",
  "main": "index.js",
  "homepage": "https://github.com/DBCDK/dbc-node-serviceprovider/tree/develop#readme",
  "scripts": {
    "test": "mocha --compilers js:babel/register src/__tests__ -w",
    "test:junit": "mocha --compilers js:babel/register src/__tests__ -R mocha-junit-reporter",
    "build": "npm run cleardest && babel ./src -d dist --ignore __tests__",
    "cleardest": "rm -rf ./dist",
    "watch": "npm run cleardest && babel ./src --watch -d dist --ignore __tests__",
    "reinstall": "rm -r node_modules && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/dbc-node-serviceprovider.git#develop"
  },
  "bugs": {
    "url": "https://github.com/DBCDK/dbc-node-serviceprovider/issues"
  },
  "keywords": [
    "dbc",
    "webservice",
    "provider",
    "ws"
  ],
  "author": "DBC A/S",
  "license": "ISC",
  "dependencies": {
    "dbc-node-moreinfo-client": "git+https://github.com/DBCDK/dbc-node-moreinfo-client#develop",
    "dbc-node-opensearch-client": "git+https://github.com/DBCDK/dbc-node-opensearch-client#develop",
    "dbc-node-opensuggest": "git+https://github.com/DBCDK/dbc-node-opensuggest#develop",
    "dbc-node-popsuggest": "git+https://github.com/DBCDK/dbc-node-popsuggest#develop",
    "lodash": "^3.9.3",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "babel": "^5.5.6",
    "chai": "^3.0.0",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.4.0",
    "node-libs-browser": "^0.5.2",
    "sinon": "^1.15.0",
    "webpack": "^1.9.10"
  }
}
